{
  "name": "orchestra_website",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types",
    "predeploy": "npm run build && wrangler deploy --dry-run --outdir .wrangler-out-server --config build/server/wrangler.json",
    "deploy": "npm run build && wrangler deploy",
    "deploy:dry-run": "wrangler deploy --dry-run --outdir .wrangler-out --config wrangler.jsonc",
    "deploy:dry-run:server": "npm run build && wrangler deploy --dry-run --outdir .wrangler-out-server --config build/server/wrangler.json",
    "deploy:server": "npm run build && wrangler deploy --config build/server/wrangler.json",
    "deploy:preview": "wrangler versions upload",
    "deploy:promote": "wrangler versions deploy",
    "dev": "react-router dev",
    "dev:worker:local": "wrangler dev --local",
    "dev:worker:remote": "wrangler dev --remote",
    "postinstall": "npm run cf-typegen",
    "preview": "npm run build && vite preview",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
    "test": "vitest run -r",
    "test:security": "vitest run tests/security.test.ts -r",
    "test:download": "vitest run tests/download.test.ts -r",
    "test:seo": "vitest run tests/seo.test.ts -r"
  },
  "dependencies": {
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.0.12",
    "@cloudflare/vitest-pool-workers": "^0.8.61",
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20.19.10",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.13.2"
  }
}
